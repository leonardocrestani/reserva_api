{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": "standard",
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
        "no-useless-constructor": "off",
        "indent": ["error", 2],
        "no-unused-vars": "off",
        "camelcase": "off",
        "array-callback-return": "off"
    }
}
